{
  "name": "b4x-language-support",
  "displayName": "B4X Language Support",
  "description": "B4x Language Support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
